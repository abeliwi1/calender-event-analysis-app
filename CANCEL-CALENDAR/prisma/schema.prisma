generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Event {
  id          String   @id
  title       String
  start       DateTime
  end         DateTime
  energy      Int      // 1–5
  value       Int      // 1–5
  score       Int
  cancel      Boolean
}
